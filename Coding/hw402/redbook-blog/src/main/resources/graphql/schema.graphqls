type Query {
    postById(id: ID): Post
    postAuthorById(id: ID): Author
}

type Mutation{
    createPost(title:String!, description:String!, content:String): Post
    createAuthor(name:String!,password:String!): Author
}

type Post{
    id: ID
    title: String
    description: String
    content: String
}

type Author {
    id: ID
    name: String
    password: String
}